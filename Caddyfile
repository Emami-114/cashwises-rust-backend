cashwises.backend.api.cwcash.de {
    reverse_proxy {
        to api-rust:8000
        transport http {
            tls {
                protocols tls1.2
                insecure_skip_verify
            }
        }
    }
    header_down Strict-Transport-Security max-age=31536000
}

smtp.strato.de:465 {
    reverse_proxy {
        to smtp.strato.de:465
        transport http {
            tls {
                protocols tls1.2
                insecure_skip_verify
            }
        }
    }
    tls info@cwcash.de
}

pgadmin4.cwcash.de {
    reverse_proxy {
        to pgadmin:80
        transport http {
            tls {
                protocols tls1.2
                insecure_skip_verify
            }
        }
    }
    header_down Strict-Transport-Security max-age=31536000
}
